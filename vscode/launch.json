  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "protocol": "inspector",
      "request": "attach",
      "name": "Attach to Backend",
      "processId": "${command:PickProcess}",
      "skipFiles": [
        "${workspaceRoot}/node_modules/**/*.js",
        "${workspaceRoot}/lib/**/*.js"
      ]
    }
  ]
}